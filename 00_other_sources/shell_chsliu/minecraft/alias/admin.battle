#!/bin/bash

#=================================
for_each_player_do() {
	players=$(mcdo list | grep.player)
	export mcecho -e_off=1
	for player in $players
	do
		# echo -e mcdo $1 $player
		mcdo $1 $player
	done
	export mcecho -e_off=
}

#=================================
alias alldo='for_each_player_do $1'
